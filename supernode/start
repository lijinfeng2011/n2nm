#!/opt/mydan/perl/bin/perl
use strict;
use warnings;

use FindBin qw( $RealBin );
use YAML::XS;

my $c = eval{ YAML::XS::LoadFile "$RealBin/../conf.yml"; };
die "load conf fail: $@" if $@;

my $port = $c->{supernode}{port};
die "port err" unless $port && $port =~ /^\d+$/;
system "supernode -p $port";
